	author = {Morris, Tim P. and White, Ian R. and Crowther, Michael J.},
	year = {2019},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/sim.8086},
	keywords = {graphics for simulation, Monte Carlo, simulation design, simulation reporting, simulation studies},
	pages = {2074--2102},
	file = {Full Text PDF:C\:\\Users\\leona\\Zotero\\storage\\7PM77YBX\\Morris et al. - 2019 - Using simulation studies to evaluate statistical m.pdf:application/pdf;Snapshot:C\:\\Users\\leona\\Zotero\\storage\\VNC4ZBLH\\sim.html:text/html},
