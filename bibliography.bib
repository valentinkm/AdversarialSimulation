	author = {Boulesteix, Anne-Laure and Groenwold, Rolf Hh and Abrahamowicz, Michal and Binder, Harald and Briel, Matthias and Hornung, Roman and Morris, Tim P and Rahnenführer, Jörg and Sauerbrei, Willi},
